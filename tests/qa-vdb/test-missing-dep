set_portroot
ebuild "$(get_ebuild libdummy 0 lib.so:"${LIBD100}")" merge
ebuild "$(get_ebuild dummy 0 bin:"${EXED}")" merge
expect "QA:*+dev-test/libdummy" -crWsbx dev-test/dummy
ebuild "$(get_ebuild dummy 1 bin:"${EXED}" RDEPEND:dev-test/libdummy)" merge
expect "QA:*expected*" -crWsbx dev-test/dummy
